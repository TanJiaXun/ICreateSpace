<!--pages/application/application.wxml-->
<view class="weui-cells__title">表单</view>
        <view class="weui-cells weui-cells_after-title">

            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">您的姓名</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入姓名" bindinput='bindNicknameChange'/>
                </view>
            </view>

            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">项目名称</view>
                </view>
                <view class="weui-cell__bd">
                    <!-- <input class="weui-input" placeholder="请输入qq"/> -->
                    <view>{{pro.proName}}</view>
                </view>
            </view>
            <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label">项目分类</view>
                </view>
                <view class="weui-cell__bd">
                    <!-- <input class="weui-input" placeholder="请输入手机号" /> -->
                    <view>{{pro.major}}</view>
                </view>
            </view>
            
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap="onsubmit" >确定</button>
            </view>
        </view>
